{
  "presets": ["env", "react", "stage-2"],
  "plugins": [
    [
      "module-resolver", {
        "root": ["./src"],
        "alias": {
          "test": "./test",
          "underscore": "lodash"
        }
      },
      "syntax-dynamic-import",
      ["import-inspector", {
        "serverSideRequirePath": true
      }],
      "react-loadable/babel",
      "dynamic-import-webpack",
      "dynamic-import-node",
      "universal-import"
    ]
  ]
}
